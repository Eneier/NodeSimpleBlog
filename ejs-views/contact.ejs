<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
<body>
  <%- include('./partials/nav.ejs') %>
  <h1>Contact:</h1>
  <ul class="contacts">
    <% if (contact.length) { %>
      <% contact.forEach(({ link, name, num}) => { %>
        <li>
          <a href="<%= link %>"><%= name %> - <%= num %></a>
        </li>
      <% }) %>
    <% } %>
  </ul>
</body>
</html>
